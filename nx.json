{
  "npmScope": "course-fe-next",
  "affected": {
    "defaultBase": "master"
  },
  "implicitDependencies": {
    "workspace.json": "*",
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    "tsconfig.base.json": "*",
    "tslint.json": "*",
    ".eslintrc.json": "*",
    "nx.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "compodoc"]
      }
    }
  },
  "projects": {
    "student-app": {
      "tags": ["scope:student-app"]
    },
    "shared-data-api-access": {
      "tags": ["load:eager", "scope:shared", "type:data-access"]
    },
    "tools-stub": {
      "tags": ["type:forbidden"]
    },
    "shared-data-course-api": {
      "tags": ["load:eager", "scope:shared", "type:data-access"]
    },
    "shared-util-rx": {
      "tags": ["load:eager", "scope:shared", "type:util"]
    }
  }
}
