        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/shared/course/data-course-api/src/**/*.ts",
              "libs/shared/course/data-course-api/src/**/*.html"
            ]
          }
        }
      }
    },
    "shared-util-etc": {
      "projectType": "library",
      "root": "libs/shared/util-etc",
      "sourceRoot": "libs/shared/util-etc/src",
      "prefix": "app",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/shared/util-etc/src/**/*.ts", "libs/shared/util-etc/src/**/*.html"]
          }
        }
      }
    },
    "login-app": {
