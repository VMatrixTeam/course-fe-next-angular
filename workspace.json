{
  "version": 1,
  "projects": {
    "student-app": {
      "projectType": "application",
      "root": "apps/student-app",
      "sourceRoot": "apps/student-app/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-builders/custom-webpack:browser",
          "options": {
            "outputPath": "dist/apps/student-app",
            "index": "apps/student-app/src/index.html",
            "main": "apps/student-app/src/main.ts",
            "polyfills": "apps/student-app/src/polyfills.ts",
            "tsConfig": "apps/student-app/tsconfig.app.json",
            "aot": true,
            "assets": ["apps/student-app/src/favicon.ico", "apps/student-app/src/assets"],
            "styles": ["apps/student-app/src/styles.css"],
            "scripts": [],
            "customWebpackConfig": {
              "path": "webpack.config.js"
            },
            "indexTransform": "index.transform.js"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/student-app/src/environments/environment.ts",
                  "with": "apps/student-app/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": true,
              "namedChunks": false,
              "extractLicenses": false,
              "vendorChunk": true,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-builders/custom-webpack:dev-server",
          "options": {
            "browserTarget": "student-app:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "student-app:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "student-app:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/student-app/src/**/*.ts", "apps/student-app/src/**/*.html"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/student-app"],
          "options": {
            "jestConfig": "apps/student-app/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "shared-data-api-access": {
      "projectType": "library",
      "root": "libs/shared/data-api-access",
      "sourceRoot": "libs/shared/data-api-access/src",
      "prefix": "app",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/shared/data-api-access/src/**/*.ts",
              "libs/shared/data-api-access/src/**/*.html"
            ]
          }
        }
      }
    },
    "tools-stub": {
      "root": "libs/tools-stub",
      "sourceRoot": "libs/tools-stub/src",
      "projectType": "library",
      "architect": {
        "compodoc": {
          "builder": "@twittwer/compodoc:compodoc",
          "options": {
            "tsConfig": "libs/tools-stub/tsconfig.compodoc.json",
            "outputPath": "dist/compodoc/tools-stub",
            "workspaceDocs": true,
            "theme": "readthedocs",
            "hideGenerator": true,
            "templates": "compodoc-templates"
          },
          "configurations": {
            "json": {
              "exportFormat": "json"
            }
          }
        }
      }
    },
    "shared-data-course-api": {
      "projectType": "library",
      "root": "libs/shared/data-course-api",
      "sourceRoot": "libs/shared/data-course-api/src",
      "prefix": "app",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/shared/data-course-api/src/**/*.ts",
              "libs/shared/data-course-api/src/**/*.html"
            ]
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "schematics": {
    "@nrwl/angular:application": {
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress",
      "strict": true,
      "linter": "eslint"
    },
    "@nrwl/angular:library": {
      "unitTestRunner": "jest",
      "strict": true,
      "linter": "eslint"
    },
    "@nrwl/angular:component": {}
  },
  "defaultProject": "student-app"
}
