            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/login-app/src/**/*.ts", "apps/login-app/src/**/*.html"]
          }
        }
      }
    },
    "login-feat-login": {
      "projectType": "library",
      "root": "libs/login/feat-login",
      "sourceRoot": "libs/login/feat-login/src",
      "prefix": "app",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/login/feat-login/src/**/*.ts", "libs/login/feat-login/src/**/*.html"]
          }
        }
      }
    },
    "shared-data-user": {
      "projectType": "library",
      "root": "libs/shared/data-user",
      "sourceRoot": "libs/shared/data-user/src",
      "prefix": "app",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/shared/data-user/src/**/*.ts", "libs/shared/data-user/src/**/*.html"]
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "schematics": {
    "@nrwl/angular:application": {
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress",
      "strict": true,
      "linter": "eslint"
    },
    "@nrwl/angular:library": {
      "unitTestRunner": "jest",
      "strict": true,
      "linter": "eslint"
    },
    "@nrwl/angular:component": {}
  },
  "defaultProject": "student-app"
}
