{
  "version": 1,
  "projects": {
    "student-app": {
      "projectType": "application",
      "root": "apps/student-app",
      "sourceRoot": "apps/student-app/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-builders/custom-webpack:browser",
          "options": {
            "outputPath": "dist/apps/student-app",
            "index": "apps/student-app/src/index.html",
            "main": "apps/student-app/src/main.ts",
            "polyfills": "apps/student-app/src/polyfills.ts",
            "tsConfig": "apps/student-app/tsconfig.app.json",
            "aot": true,
            "assets": ["apps/student-app/src/favicon.ico", "apps/student-app/src/assets"],
            "styles": [
              "apps/student-app/src/styles.css",
              {
                "input": "node_modules/tailwindcss/base.css",
                "bundleName": "tailwindcss"
              },
              {
                "input": "node_modules/tailwindcss/components.css",
                "bundleName": "tailwindcss"
              },
              {
                "input": "node_modules/tailwindcss/utilities.css",
                "bundleName": "tailwindcss"
              },
              {
                "input": "node_modules/primeicons/primeicons.css",
                "bundleName": "primeng"
              },
              {
                "input": "node_modules/primeng/resources/primeng.min.css",
                "bundleName": "primeng"
              },
              {
                "input": "node_modules/primeng/resources/themes/saga-green/theme.css",
                "bundleName": "primeng"
              }
            ],
            "scripts": [],
            "customWebpackConfig": {
              "path": "webpack.config.js"
            },
            "indexTransform": "index.transform.js"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/student-app/src/environments/environment.ts",
                  "with": "apps/student-app/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": true,
              "namedChunks": false,
              "extractLicenses": true,
              "aot": true,
              "vendorChunk": true,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-builders/custom-webpack:dev-server",
          "options": {
            "browserTarget": "student-app:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "student-app:build:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/student-app/src/**/*.ts", "apps/student-app/src/**/*.html"]
          }
        }
      }
    },
    "shared-data-api-access": {
      "projectType": "library",
      "root": "libs/shared/data-api-access",
      "sourceRoot": "libs/shared/data-api-access/src",
      "prefix": "app",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/shared/data-api-access/src/**/*.ts", "libs/shared/data-api-access/src/**/*.html"]
          }
        }
      }
    },
    "tools-stub": {
      "root": "libs/tools-stub",
      "sourceRoot": "libs/tools-stub/src",
      "projectType": "library",
      "architect": {
        "compodoc": {
          "builder": "@twittwer/compodoc:compodoc",
          "options": {
            "tsConfig": "libs/tools-stub/tsconfig.compodoc.json",
            "outputPath": "dist/compodoc/tools-stub",
            "workspaceDocs": true,
            "theme": "readthedocs",
            "hideGenerator": true,
            "templates": "compodoc-templates"
          },
          "configurations": {
            "json": {
              "exportFormat": "json"
            }
          }
        }
      }
    },
    "shared-ui-async": {
      "projectType": "library",
      "root": "libs/shared/ui-async",
      "sourceRoot": "libs/shared/ui-async/src",
      "prefix": "app",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/shared/ui-async/src/**/*.ts", "libs/shared/ui-async/src/**/*.html"]
          }
        }
      }
    },
    "shared-util-pipe": {
      "projectType": "library",
      "root": "libs/shared/util-pipe",
